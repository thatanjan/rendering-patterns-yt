<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>API Data as HTML</title>
</head>
<body>
    <h1>Posts from API</h1>
    <ul>
        <% posts.forEach(function(post) { %>
            <li>
                <strong><a href="/post/<%= post.id %>"><%= post.title %></a></strong>
                <p><%= post.body %></p>
            </li>
        <% }); %>
    </ul>
</body>
</html>
